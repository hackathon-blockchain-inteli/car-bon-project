	"scripts": {
		"build": "npx tsc",
		"start": "node ./dist/app.js",
		"dev": "concurrently \"npx tsc --watch\" \"nodemon -q ./dist/app.js\""
	},
	"type": "commonjs",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hackathon-blockchain-inteli/car-bon-project.git"
	},
