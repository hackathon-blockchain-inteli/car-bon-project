	},
	"homepage": "https://github.com/hackathon-blockchain-inteli/car-bon-project#readme",
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"ethers": "^6.3.0",
		"express": "^4.18.2",
		"mqtt": "^4.3.7"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/node": "^20.1.0",
